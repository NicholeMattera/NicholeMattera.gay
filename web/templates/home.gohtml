    <body>
{{ if .Pride }}
        <video autoplay loop muted poster="/static/images/prideBackground.jpg" src="/static/videos/prideBackground.m4v"></video>
{{ else }}
        <video autoplay loop muted poster="/static/images/background.jpg" src="/static/videos/background.m4v"></video>
{{ end }}

